        {
          "name": "Programmatic Keywords",
          "keys": [
            {
              "name": "TITLE",
              "title": "Proposal title",
              "description": "Proposal title. The proposal title can have a length of up to 150 characters. This is longer than the 68 characters available to a single FITS keyword value. The TITLE keyword will use the FITS long string value convention to end incomplete values with an ampersand and continue the value in next record with a CONTINUE element.",
              "attributes": [
                {
                  "name": "type",
                  "value": "string"
                },
                {
                  "name": "sql_dtype",
                  "value": "nvarchar(150)"
                },
                {
                  "name": "calculation",
                  "value": ""
                },
                {
                  "name": "default_value",
                  "value": "UNKNOWN"
                },
                {
                  "name": "example",
                  "value": "Cryo2 SIC NIRISS"
                },
                {
                  "name": "units",
                  "value": ""
                },
                {
                  "name": "sw_source",
                  "value": "PPS:dms_program_view.title"
                },
                {
                  "name": "source",
                  "value": "Proposal and Planning System (PPS)"
                },
                {
                  "name": "level",
                  "value": "1b"
                },
                {
                  "name": "si",
                  "value": "Multiple"
                },
                {
                  "name": "section",
                  "value": "Programmatic Keywords"
                },
                {
                  "name": "mode",
                  "value": "All"
                },
                {
